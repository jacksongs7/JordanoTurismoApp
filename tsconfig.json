{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./app/src/*"
      ],
      "@/app/*": [
        "./app/*"
      ],
      "@/app/_lib/components/*": [
        "./app/_lib/components/*"
      ],
      "@/app/_lib/data/*": [
        "./app/_lib/data/*"
      ],
      "@/app/_lib/services/*": [
        "./app/_lib/services/*"
      ],
      "@/app/_lib/types/*": [
        "./app/_lib/types/*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}

